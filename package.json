{
  "name": "habit-tracker",
  "version": "1.0.0",
  "description": "A modern habit tracking application with calendar view",
  "main": "index.js",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm start",
    "backend": "cd backend && uvicorn main:app --reload",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "k8s:deploy": "cd k8s && ./deploy.sh",
    "test": "cd frontend && npm test && cd ../backend && pytest",
    "lint": "cd frontend && npm run lint",
    "format": "cd backend && black . && isort ."
  },
  "keywords": ["habit-tracker", "calendar", "productivity", "fastapi", "nextjs", "kubernetes", "docker"],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/habit-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/habit-tracker/issues"
  },
  "homepage": "https://github.com/yourusername/habit-tracker#readme"
}
