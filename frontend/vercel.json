{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://tommy-habit-tracker.up.railway.app/$1"
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_URL": "https://tommy-habit-tracker.up.railway.app"
    }
  }
  