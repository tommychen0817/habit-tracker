{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "$NEXT_PUBLIC_API_URL/$1"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_URL": "@NEXT_PUBLIC_API_URL"
    }
  }
  